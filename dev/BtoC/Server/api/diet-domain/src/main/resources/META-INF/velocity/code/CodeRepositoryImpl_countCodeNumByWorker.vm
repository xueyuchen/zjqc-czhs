SELECT
    count(*) AS count
FROM
    worker w
INNER JOIN code c ON w.worker_id = c.worker_id
AND c.cre_dt > :fromDt
AND c.cre_dt < :toDt
AND w.worker_name = :workerName
AND w.[local] = :local
GROUP BY
    w.worker_id,
    w.worker_name,
    c.cre_dt,
    w.[local]
ORDER BY
        w.worker_name,
        c.cre_dt